{
  "name": "iobroker.octoprint",
  "version": "5.1.0",
  "description": "ioBroker OctoPrint Adapter",
  "author": {
    "name": "Matthias Kleine",
    "email": "info@haus-automatisierung.com",
    "url": "https://haus-automatisierung.com/"
  },
  "homepage": "https://github.com/klein0r/ioBroker.octoprint",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "Smart Home",
    "home automation",
    "octoprint",
    "3d printing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klein0r/ioBroker.octoprint"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.1.6",
    "axios": "^1.7.8"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.8.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.7.2",
    "@alcalzone/release-script-plugin-license": "^3.7.0",
    "@alcalzone/release-script-plugin-manual-review": "^3.7.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.11.1",
    "@iobroker/adapter-dev": "^1.3.0",
    "@iobroker/testing": "^4.1.3",
    "@types/chai": "^4.3.19",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.8",
    "@types/node": "^22.10.1",
    "@types/proxyquire": "^1.3.31",
    "@types/sinon": "^17.0.3",
    "@types/sinon-chai": "^3.2.12",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "mocha": "^10.7.3",
    "prettier": "^3.4.1",
    "proxyquire": "^2.1.3",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "~5.7.2"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint",
    "translate": "translate-adapter",
    "release": "release-script",
    "release-patch": "release-script patch --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes"
  },
  "bugs": {
    "url": "https://github.com/klein0r/ioBroker.octoprint/issues"
  },
  "readmeFilename": "README.md"
}
