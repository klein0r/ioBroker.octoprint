{
  "name": "iobroker.octoprint",
  "version": "2.0.5",
  "description": "ioBroker OctoPrint Adapter",
  "author": {
    "name": "Matthias Kleine",
    "email": "info@haus-automatisierung.com",
    "url": "https://haus-automatisierung.com/"
  },
  "homepage": "https://github.com/klein0r/ioBroker.octoprint",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "Smart Home",
    "home automation",
    "octoprint",
    "3d printing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/klein0r/ioBroker.octoprint"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^2.5.1",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.4.1",
    "@alcalzone/release-script-plugin-iobroker": "^3.4.1",
    "@alcalzone/release-script-plugin-license": "^3.4.1",
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@iobroker/adapter-react": "2.0.13",
    "@iobroker/testing": "^2.5.2",
    "@material-ui/core": "^4.12.3",
    "@types/chai": "^4.2.22",
    "@types/chai-as-promised": "^7.1.4",
    "@types/gulp": "^4.0.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.32",
    "@types/proxyquire": "^1.3.28",
    "@types/react-dom": "^16.9.14",
    "@types/react": "^16.14.20",
    "@types/sinon": "^10.0.6",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.26.1",
    "gulp": "^4.0.2",
    "mocha": "^9.1.3",
    "parcel-bundler": "^1.12.5",
    "proxyquire": "^2.1.3",
    "react-dom": "^16.14.0",
    "react": "^16.14.0",
    "sinon": "^12.0.1",
    "sinon-chai": "^3.7.0",
    "typescript": "~4.4.4"
  },
  "main": "main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "lib/",
    "www/",
    "io-package.json",
    "LICENSE",
    "main.js"
  ],
  "scripts": {
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint": "eslint --ext .js,.jsx",
    "release": "release-script patch --yes",
    "release-minor": "release-script minor --yes",
    "release-major": "release-script major --yes"
  },
  "bugs": {
    "url": "https://github.com/klein0r/ioBroker.octoprint/issues"
  },
  "readmeFilename": "README.md"
}
